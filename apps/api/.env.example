# Solana Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_NETWORK=devnet

# House Wallet (generate with: solana-keygen new --outfile house-wallet.json)
# Then get the private key array and convert to base64
HOUSE_WALLET_PRIVATE_KEY=your-base64-encoded-private-key-here

# JWT Secret (generate random string)
JWT_SECRET=your-random-secret-string-min-32-chars-long

# Server Configuration
PORT=3001
NODE_ENV=development

# Database
DATABASE_PATH=./data/casino.db
